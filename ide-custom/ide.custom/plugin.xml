<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

	<extension point="org.eclipse.ui.startup">
		<startup class="ide.custom.ForcePerspectiveOnStart"/>
	</extension>
	
	<extension point="org.eclipse.ui.perspectives">
		<perspective
			class="ide.custom.CustomPerspective"
			icon="icons/perspective.png"
			id="ide.custom.perspective"
			name="Custom Perspective"/>
	</extension>
	
</plugin>
